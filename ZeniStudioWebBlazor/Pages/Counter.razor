@page "/counter"

<PageTitle>Proyectos</PageTitle>

<h1>Proyectos</h1>

<p role="status">Current count: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Cotiza</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
